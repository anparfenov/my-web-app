{
    "name": "my-site",
    "version": "0.0.1",
    "scripts": {
        "dev": "npm run build:cache && ts-node-dev --respawn server",
        "build": "npm run build:cache && npm run build:client && npm run build:entry-server && npm run build:server",
        "build:cache": "node server/cache/build.js",
        "build:client": "vite build --ssrManifest --outDir dist/client",
        "build:entry-server": "vite build --ssr src/entry-server.ts --outDir dist/server",
        "build:server": "tsc -p tsconfig.server.json",
        "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
        "serve": "cross-env NODE_ENV=production node dist/ts-dist/server",
        "debug": "node --inspect-brk server",
        "lint": "eslint .",
        "test": "npm run test:unit",
        "test:unit": "jest"
    },
    "dependencies": {
        "@nuxt/devalue": "^2.0.0",
        "@popperjs/core": "^2.10.2",
        "@types/js-yaml": "^4.0.4",
        "animejs": "^3.2.1",
        "axios": "^0.24.0",
        "cross-env": "^7.0.3",
        "date-fns": "^2.25.0",
        "fastify": "^3.23.0",
        "fastify-compress": "^3.6.1",
        "fastify-sensible": "^3.1.2",
        "fastify-static": "^4.5.0",
        "fp-ts": "^2.11.5",
        "fs-extra": "^10.0.0",
        "js-yaml": "^4.1.0",
        "markdown-it": "^12.2.0",
        "markdown-it-anchor": "^8.4.1",
        "markdown-it-front-matter": "^0.2.3",
        "middie": "^5.3.0",
        "nanoid": "^3.1.30",
        "node-fetch": "^3.0.0",
        "pinia": "^2.0.6",
        "vue": "^3.2.16",
        "vue-i18n": "^9.2.0-beta.17",
        "vue-router": "^4.0.12",
        "vue-tabler-icons": "^2.1.0",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@types/connect": "^3.4.35",
        "@types/fs-extra": "^9.0.13",
        "@types/node": "^16.11.6",
        "@vitejs/plugin-vue": "^1.9.3",
        "@vue/compiler-sfc": "^3.2.21",
        "@vue/server-renderer": "^3.2.21",
        "autoprefixer": "^10.4.0",
        "eslint": "^8.1.0",
        "eslint-plugin-vue": "^8.0.3",
        "pino-pretty": "^7.2.0",
        "postcss": "^8.3.11",
        "postcss-custom-media": "^8.0.0",
        "postcss-nested": "^5.0.6",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.0-dev.20211226",
        "vite": "^2.6.4",
        "vue-tsc": "^0.3.0"
    }
}
